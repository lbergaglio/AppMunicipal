<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="#FF9800">

    <FrameLayout
        android:id="@+id/frameLayout3"
        android:layout_width="409dp"
        android:layout_height="120dp"
        android:background="#35FF9800"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="250dp"
            android:layout_height="80dp"
            android:translationX="14.5dp"
            android:translationY="20dp"
            app:srcCompat="@drawable/piclogomunicipio" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" />

        <ImageButton
            android:id="@+id/AccountButton"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:background="#00FF9800"
            android:scaleType="fitCenter"
            android:translationX="309.5dp"
            android:translationY="20dp"
            app:srcCompat="@drawable/cuenta"
            app:tint="#FF9800" />

    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout2"
        android:layout_width="409dp"
        android:layout_height="100dp"
        android:background="#35FF9800"
        android:translationX="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <ImageButton
            android:id="@+id/homeButton"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:background="#00FF9800"
            android:scaleType="fitCenter"
            android:translationX="14.5dp"
            android:translationY="15dp"
            app:srcCompat="@drawable/casa"
            app:tint="#FF9800" />

        <ImageButton
            android:id="@+id/telefoneButton"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:background="#00FF9800"
            android:scaleType="fitCenter"
            android:translationX="324.5dp"
            android:translationY="15dp"
            app:srcCompat="@drawable/denuncia"
            app:tint="#FF9800" />

        <ImageButton
            android:id="@+id/reportButton"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:background="#00FF9800"
            android:scaleType="fitCenter"
            android:translationX="117.83dp"
            android:translationY="15dp"
            app:srcCompat="@drawable/reporte"
            app:tint="#FF9800" />

        <ImageButton
            android:id="@+id/workButton"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:background="#00FF9800"
            android:scaleType="fitCenter"
            android:translationX="221.11dp"
            android:translationY="15dp"
            app:srcCompat="@drawable/maleta"
            app:tint="#FF9800" />
    </FrameLayout>

    <Button
        android:id="@+id/button6"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:backgroundTint="#FF9800"
        android:text="Cambiar Contraseña"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="400dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button7"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:backgroundTint="#FF9800"
        android:text="Mis Servicios"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="300dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button8"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:backgroundTint="#FF9800"
        android:text="Cerrar Sesión"

        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="500dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button9"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:backgroundTint="#FF9800"
        android:text="Mi Perfil"

        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="200dp"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>