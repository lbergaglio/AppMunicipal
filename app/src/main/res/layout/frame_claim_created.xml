            android:translationX="117.83dp"
            android:translationY="15dp"
            app:srcCompat="@drawable/reporte"
            app:tint="#FF9800" />

        <ImageButton
            android:id="@+id/workButton"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:background="#00FF9800"
            android:scaleType="fitCenter"
            android:translationX="221.11dp"
            android:translationY="15dp"
            app:srcCompat="@drawable/maleta"
            app:tint="#FF9800" />
    </FrameLayout>
